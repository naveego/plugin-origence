{
  "id": "plugin-origence",
  "displayName": "Origence",
  "description": "Publisher which connects to Origence",
  "apiVersion": "2",
  "versionDescription": "Version 1",
  "version": "1.0.0",
  "iconFile": "icon.png",
  "executable": "PluginOrigence",
  "kind": "publisher",
  "os": "$GOOS",
  "arch": "$GOARCH",
  "accessGroups": [],
  "canConfigure": true,
  "canProduceMultipleSchemas": true,
  "canAcceptQueryBasedSchemas": false,
  "canUseOAuth": false,
  "canConfigureWrite": false,
  "canWrite": false,
  "canConfigureReplication": false,
  "canWriteReplication": false,
  "querySchema": {
    "ui": {
      "ui:widget": "code",
      "ui:options": {
        "language": "sql"
      }
    },
    "schema": {
      "type": "string"
    }
  },
  "configSchema": {
    "ui": {
      "ui:order": [
        "Hostname",
        "Port",
        "UseSftp",
        "Username",
        "Password",
        "SshKey",
        "RootPath"
      ],
      "Password": {
        "ui:widget": "password"
      },
      "SshKey": {
        "ui:widget": "password"
      }
    },
    "schema": {
      "type": "object",
      "properties": {
        "Hostname": {
          "type": "string",
          "title": "Hostname",
          "description": "Hostname for any root path using FTP/SFTP.",
          "default": ""
        },
        "Port": {
          "type": "number",
          "title": "Port",
          "description": "Port for any root path using FTP/SFTP.",
          "default": 22
        },
        "UseSftp": {
          "type": "boolean",
          "title": "Use SFTP",
          "description": "Whether to use SFTP (true) or FTP (false).",
          "default": true
        },
        "Username": {
          "type": "string",
          "title": "Username",
          "description": "Username for any root path using FTP/SFTP.",
          "default": ""
        },
        "Password": {
          "type": "string",
          "title": "Password",
          "description": "Password for any root path using FTP/SFTP.",
          "default": ""
        },
        "SshKey": {
          "type": "string",
          "title": "SFTP SSH Key",
          "description": "SSH private key for any root path using SFTP.",
          "default": ""
        },
        "RootPath": {
          "type": "string",
          "title": "Root Path",
          "description": "Origence file path on the FTP/SFTP server.",
          "default": ""
        }
      },
      "required": [
        "Hostname",
        "Port",
        "UseSftp",
        "Username",
        "RootPath"
      ]
    }
  }
}